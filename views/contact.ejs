<%- include('header'); -%>

<!-- Start Breadcrump Area  -->
<div
  class="rn-page-title-area pt--120 pb--190 pb_md--100 pb_sm--100 bg_image bg_image--5"
  data-black-overlay="5"
>
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="rn-page-title text-center pt--100">
          <h2 class="title theme-gradient">CONTACT US</h2>
          <p>Fresh From The Press Discription</p>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- End Breadcrump Area  -->
<main class="page-wrapper">
  <!-- Start Contact Area  -->
  <div class="rn-contact-area rn-section-gap bg_color--1">
    <div class="contact-form--1">
      <div class="container">
        <div class="row row--35 align-items-start">
          <div class="col-lg-6 order-2 order-lg-1">
            <div class="section-title text-start mb--50 mb_sm--30 mb_md--30">
              <h2 class="title">Contact Us.</h2>
              <p class="description">
                I am available for freelance work. Connect with me via phone:
                <a href="tel:+919654277866">9654277866</a> or email:
                <a href="mailto:support@digitalhoppin.com">
                  support@digitalhoppin.com</a
                >
              </p>
            </div>
            <div class="form-wrapper">
              <form
                class="needs-validation"
                novalidate
                id="contact-form-active"
                action="/contact-mail"
                method="POST"
                enctype="application/x-www-form-urlencoded"
              >
                <input
                  class="input"
                  name="name"
                  type="text"
                  placeholder="Your Name *"
                />
                <input
                  class="input"
                  name="email"
                  type="email"
                  placeholder="Your email *"
                />
                <input
                  class="input"
                  name="subject"
                  type="text"
                  placeholder="Write a Subject"
                />
                <textarea
                  class="input"
                  name="message"
                  placeholder="Your Message"
                ></textarea>
                <button
                  type="submit"
                  class="rn-button-style--2 btn_solid submit-btn"
                >
                  <span>Send message</span>
                </button>
              </form>
              <div class="form-output">
                <p
                  style="display: none"
                  class="form-messege-active form-messege"
                >
                  Message sent
                </p>
              </div>
            </div>
          </div>
          <div class="col-lg-6 order-1 order-lg-2">
            <div class="thumbnail mb_md--40 mb_sm--40">
              <img src="assets/images/about/about-6.jpg" alt="trydo" />
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- End Contact Area  -->
</main>
<!-- End Page Wrapper  -->
<script src="dashboard-assets/assets/js/jquery.min.js"></script>

<%- include('footer'); -%>
